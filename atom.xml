---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  
  <title>s1n4 (Sina Samavati)</title>
  <link href="http://s1n4.com/atom.xml" rel="self"/>
  <link href="http://s1n4.com/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://s1n4.com/</id>
  <author>
    <name>s1n4 (Sina Samavati)</name>
    <email>contact@s1n4.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://s1n4.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://s1n4.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
  
</feed>